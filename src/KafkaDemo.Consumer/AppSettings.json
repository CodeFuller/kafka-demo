{
  "kafka": {
    "server": "localhost:9092",
    "ssl": {
      "isEnabled": false,
      "truststoreCertLocation": null,
      "isTruststoreCertVerification": null,
      "keystoreCertLocation": null,
      "keystoreKeyLocation": null
    }
  },

  "demoConsumer": {
    "kafkaEndpoint": {
      "topic": {
        "name": "demo-topic",
        "consumer": "demo-consumer"
      },

      "checkpoint": {
        "interval": "00:01:00",
        "messages": 5000
      },

      "scalability": {
        "concurrentConsumers": 2
      }
    }
  }
}
